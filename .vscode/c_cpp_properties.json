{
	"configurations": [
		{
			"name": "CMake",
			"compileCommands": "${config:cmake.buildDirectory}/compile_commands.json",
			"configurationProvider": "ms-vscode.cmake-tools",
			"includePath": [
				"${default}",
				"${workspaceFolder}/src/inknit/inc",
				"${workspaceFolder}/src/inknit_cxx/inc"
			],
			"cStandard": "c17",
			"cppStandard": "c++20",
			"defines": [
				"INKNIT_ENABLE_BENCHMARKS",
				"INKNIT_ENABLE_EXPORTS"
			]
		}
	],
	"version": 4
}
