{
	"files.associations": {
		"*.c.in": "c",
		"*.cpp.in": "cpp",
		"*.h": "c",
		"*.h.in": "c",
		"*.hpp": "cpp",
		"*.hpp.in": "cpp"
	},
	"remote.autoForwardPorts": false,
	"remote.restoreForwardedPorts": false,
	"remote.portsAttributes": {
		"3333": {
			"label": "Application",
			"onAutoForward": "ignore"
		}
	},
	"search.exclude": {
		".git/": true,
		"build/": true,
	},
	"workbench.editor.customLabels.patterns": {
		"*/{benchmarks,src,sources,tests}/**/*": "[${dirname(-2)}] ${filename}.${extname}"
	}
}
